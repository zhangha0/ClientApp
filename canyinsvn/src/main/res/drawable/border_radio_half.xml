<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" >
    <!-- 绘制图形的颜色 -->
    <solid android:color="#ffffff"/>
    <!-- 绘制边角弧度 -->
    <corners android:radius="30px"/>
    <!-- 描边 -->
    <stroke android:color="#ffffff"
        android:width="1px"/>
    <!-- 设定内边距 -->
    <padding android:left="0px"
        android:right="0px" />
</shape>